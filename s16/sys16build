#!/bin/bash

node ../s16a/s16a --asm asm/sys16.s16a --out asm/sys16.s16o

echo -en "\n";

if [ "$?" -ne 0 ]; then
    echo -en "\nThere was an error...\n";
    exit 1;
fi

if [ "$#" -ne 1 ]; then
    ENDIANESS='little-endian';
else
    ENDIANESS=$1;
fi

