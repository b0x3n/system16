    .section        code


///////////////////////////////////////////////////////////
//  __monitor_handler                                    //
///////////////////////////////////////////////////////////
//
        function    __monitor_handler

            push16  %BP;
            mov16   BP, %SP;

            mov8   FX, %OI;
            //mov8    EX, #AX;

            pop16   BP;

            ret     0;

        end


///////////////////////////////////////////////////////////
//  __keyboard_handler                                   //
///////////////////////////////////////////////////////////
//
        function    __keyboard_handler

            push16  %BP;
            mov16   BP, %SP;

            mov8   FX, %OI;

            pop16   BP;

            ret     0;

        end

